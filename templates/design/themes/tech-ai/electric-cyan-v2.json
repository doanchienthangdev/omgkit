  "effects": {
    "glassMorphism": {
      "background": { "$ref": "scales.neutral.steps.light.1" },
      "backdropBlur": "12px"
    },
    "glow": {
      "default": "0 0 20px",
      "lg": "0 0 40px",
      "color": { "$ref": "scales.primary.steps.light.9" }
    },
    "gradient": {
